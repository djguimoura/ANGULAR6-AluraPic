esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../pixelmatch/bin/pixelmatch" "$@"
  ret=$?
else 
  node  "$basedir/../pixelmatch/bin/pixelmatch" "$@"
  ret=$?
fi
exit $ret
